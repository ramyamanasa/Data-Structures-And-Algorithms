class Solution {
public:
    vector<vector<int>> subsets(vector<int>& nums) 
    {
        vector<vector<int>> ans;
        vector<int> v;
        subset(0,nums,v,ans);
        return ans;
    }
    void subset(int j,vector<int>& nums,vector<int>& v,vector<vector<int>>&ans)
    {
        ans.push_back(v);
        for(int i=j;i<nums.size();i++)
        {
            v.push_back(nums[i]);
            subset(i+1,nums,v,ans);
            v.pop_back();
        }
    }
};
